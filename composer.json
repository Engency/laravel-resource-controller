{
  "name": "engency/resource-controllers",
  "description": "Resource Controllers for Laravel",
  "keywords": [
    "laravel",
    "resource",
    "controller"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Engency",
      "homepage": "https://www.engency.com"
    },
    {
      "name": "Frank Kuipers",
      "email": "frank@engency.com"
    }
  ],
  "require": {
    "php": "^7.4",
    "illuminate/http": "^8.0",
    "illuminate/routing": "^8.0",
    "illuminate/database": "^8.0",
    "engency/eloquent-formatting": "^0.1.0",
    "engency/laravel-model-validation": "^0.1.0",
    "illuminate/view": "^8.10"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5",
    "phpunit/phpunit": "^9.4",
    "mockery/mockery": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Engency\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Engency\\Test\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Engency\\Providers\\ResourceServiceProvider"
      ]
    },
    "branch-alias": {
      "dev-master": "0.1-dev"
    }
  },
  "scripts": {
    "check-style": "phpcs -p --standard=psr2 src/",
    "fix-style": "phpcbf -p --standard=psr2 src/"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
